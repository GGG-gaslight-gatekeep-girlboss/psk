        <RootNamespace>CoffeeShop.DataAccess</RootNamespace>
        <AssemblyName>CoffeeShop.DataAccess</AssemblyName>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="9.0.4">
